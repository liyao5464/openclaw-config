# 环境变量配置
# 复制此文件为 .env 并填入真实值

# Telegram Bot Tokens（从 @BotFather 获取）
BOT_MAIN_TOKEN=your_main_bot_token_here
BOT_DIRECTOR_TOKEN=your_director_bot_token_here
BOT_NANNY_TOKEN=your_nanny_bot_token_here
BOT_VIDEO_TOKEN=your_video_bot_token_here

# 飞书配置
FEISHU_APP_ID=your_feishu_app_id
FEISHU_APP_SECRET=your_feishu_secret

# 微信公众号配置
WECHAT_APP_ID=your_wechat_app_id
WECHAT_APP_SECRET=your_wechat_secret

# FAL API Key（图片生成）
FAL_KEY=your_fal_key_here

# 其他API Keys
OPENAI_API_KEY=your_openai_key_here
KIMI_API_KEY=your_kimi_key_here
